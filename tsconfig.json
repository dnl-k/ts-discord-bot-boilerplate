{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"compilerOptions": {
		"declaration": false,
		"declarationMap": false,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ES2022",
		"outDir": "dist",
		"strict": true,
		"lib": [
			"esnext",
			"ESNext.Disposable"
		],
		"skipLibCheck": true
	}
}